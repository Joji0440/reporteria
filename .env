# Variables de entorno para la aplicación de reportería

# Base de datos PostgreSQL
DATABASE_URL=postgresql://postgres@localhost:5432/banco_de_alimentos
DB_HOST=localhost
DB_PORT=5432
DB_NAME=banco_de_alimentos
DB_USER=postgres
DB_PASSWORD=

# Configuración del servidor
PORT=3001
NODE_ENV=development

# JWT Secret para autenticación
JWT_SECRET=banco_alimentos_secret_2025_reporteria
JWT_EXPIRES_IN=24h

# Configuración de archivos
UPLOADS_DIR=./uploads
EXPORTS_DIR=./exports
MAX_FILE_SIZE=10485760

# CORS
FRONTEND_URL=http://localhost:3000

# Configuración de reportes
DEFAULT_REPORTS_LIMIT=1000
REPORTS_CACHE_TTL=300
